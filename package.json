{
  "name": "modapi",
  "version": "0.0.1",
  "description": "modapi",
  "main": "dist/modapi.cjs.js",
  "module": "dist/modapi.es.js",
  "types": "types/index.d.ts",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/modapi.es.js",
      "require": "./dist/modapi.cjs.js",
      "types": "./types/index.d.ts"
    }
  },
  "homepage": "https://github.com/yuanzhhh/modapi",
  "repository": {
    "type": "git",
    "url": "https://github.com/yuanzhhh/modapi"
  },
  "keywords": [
    "modapi"
  ],
  "author": {
    "name": "evan",
    "email": "evan.yuanzh@gmail.com"
  },
  "license": "MIT",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.js",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "rollup": "^4.9.1",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
